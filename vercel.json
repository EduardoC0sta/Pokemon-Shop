{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "api/api.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/api.js"
    },
    {
      "source": "/(.*)",
      "destination": "/server.ts"
    }
  ]
}
